\ProvidesPackage{notestyle}

% **** Note specified command.

% Use these for theorems, lemmas, proofs, etc.
\theoremstyle{plain} % default
\newtheorem{theorem}{Theorem}[subsection]
\newtheorem{lemma}[theorem]{Lemma}
\newtheorem{proposition}[theorem]{Proposition}
\newtheorem{claim}[theorem]{Claim}
\newtheorem{corollary}[theorem]{Corollary}

\theoremstyle{definition}
\newtheorem{definition}[theorem]{Definition}

% Use these for algorithm analysis.
\newcommand*{\question}{\vspace{0.1in}\noindent \textbf{Question:}\ }
\newcommand*{\analysis}{\vspace{0.1in}\noindent \textbf{Analysis:}\ }
\newcommand*{\observation}{\vspace{0.1in}\noindent \textbf{Observation:}\ }
\newcommand*{\Note}{\vspace{0.1in}\noindent \textbf{Note:}\ }
\newcommand*{\AlgoInput}{\vspace{0.1in}\noindent \textbf{Input:}\ }
\newcommand*{\AlgoOutput}{\vspace{0.1in}\noindent \textbf{Output:}\ }
\newcommand*{\AlgoExample}{\vspace{0.1in}\noindent \textbf{Example:}\ }

\newcommand{\ProblemDescription}{\vspace{0.1in}\textbf{Problem Description:}\ }
